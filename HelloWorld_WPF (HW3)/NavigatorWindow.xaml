<Window x:Class="HelloWorld.NavigatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorld"
        mc:Ignorable="d"
        Title="NavigatorWindow" Height="450" Width="466.866">
    <Window.Background>
        <ImageBrush ImageSource="images/wpf-world.png" />
    </Window.Background>
    <StackPanel>
        <Label>Enter an address</Label>
        <TextBox x:Name="uxAddress">https://www.reddit.com/</TextBox>
            <Button x:Name="uxGo" Content="Go" Click="UxGo_Click"/>
    </StackPanel>
</Window>
